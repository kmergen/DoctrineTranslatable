{
    "name": "kmergen/doctrine-translatable",
    "description": "Doctrine Translatable Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "behaviors",
        "doctrine",
        "translatable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Klaus Mergen"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/persistence": "^2.5|^3.0",
        "doctrine/dbal": "^3.3",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.11",
        "symfony/dependency-injection": "7.3.*",
        "symfony/http-kernel": "7.3.*",
        "symfony/security-core": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/string": "7.3.*",
        "symfony/translation-contracts": "^3.4",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "rector/rector": "^1.2",
        "ext-pdo_mysql": "*",
        "psr/log": "^1.1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Kmergen\\DoctrineTranslatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kmergen\\DoctrineTranslatable\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    }
}